a=input("enter a buy name: ")
b=input("enter a girl name:")
a1=list(a)
b1=list(b)
for i in range(len(a)):
    for j in range(len(b)):
        if(a1[i]==b1[j]):
            a1[i]='2'
            b1[j]='2'
            print(a1)
            print(b1)
        C=0
for i in a1:
    if(i!='2'):
        C=C+1
for i in b1:
    if(j!='2'):
        C=C+1
